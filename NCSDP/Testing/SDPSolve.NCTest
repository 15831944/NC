<< SDP`
<< SDPFlat`
<< SDPSeDuMi`

Module[
  {test, k,
   A, b, y, sdp,
   X, Y, S, iters,
   AA, BB, CC
  },

  k = 1;
  test = "SDPSolve";

  A = {y0 - 2, {{y1, y0}, {y0, 1}}, {{y2, y1}, {y1, 1}}};
  b = y2;
  y = {y0, y1, y2};

  {AA,BB,CC} = SDPMatrices[b, A, y];

  {Y, X, S, iters} = SDPSolve[{AA, BB, CC}];
  answer = 16;
  exp = -SDPInner[BB, Y];
  NCTest[Abs[(exp - answer)/answer] < 10^(-5), True, test, k++];

  exp = -SDPInner[CC, X];
  NCTest[Abs[(exp - answer)/answer] < 10^(-5), True, test, k++];

  {AA,BB,CC} = SDPImport["data/arch0"];

  {Y, X, S, iters} = SDPSolve[{AA, BB, CC}];
  answer = 0.56652;
  exp = -SDPInner[BB, Y];
  NCTest[Abs[(exp - answer)/answer] < 10^(-5), True, test, k++];

  exp = -SDPInner[CC, X];
  NCTest[Abs[(exp - answer)/answer] < 10^(-5), True, test, k++];

  {AA,BB,CC} = SDPImport["data/control07"];

  {Y, X, S, iters} = SDPSolve[{AA, BB, CC}];
  answer = 126.734;
  exp = -SDPInner[BB, Y];
  NCTest[Abs[(exp - answer)/answer] < 10^(-3), True, test, k++];

  exp = -SDPInner[CC, X];
  NCTest[Abs[(exp - answer)/answer] < 10^(-5), True, test, k++];

];
