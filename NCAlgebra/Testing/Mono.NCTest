Module[ 
  {k,s,x,i},

  k = 1;

  NCTest[NCMonomial[s^2], s**s, "Mono", k++];
  NCTest[NCMonomial[x^-3], inv[x]**inv[x]**inv[x], "Mono", k++];
  NCTest[NCMonomial[x^2+x-x^0+x^-1 ], x**x+x+inv[x]-1, "Mono", k++];
  NCTest[NCMonomial[(x^0)**(x^1)**(x^2)**((inv[x])^3)], 1, "Mono", k++];

];
