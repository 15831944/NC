Module[
   {k, test, exp, answer, tmp,
   a,b,c,d,e,f,g,h,j,x,y,z,A,B,C},

   k = 1;
   test = "Collect";

   SetNonCommutative[a,b,c,d,e,f,g,h,j,x,y,z];
   SetCommutative[A,B,C];

   exp = x**y + x;
   answer = x**(1+y);
   NCTest[NCStrongCollectNew[exp,x], answer, test, k++];
   NCTest[NCStrongCollectNew[exp,{x}], answer, test, k++];

   exp = A x**y + B x;
   answer = x**(B + A y);
   NCTest[NCStrongCollectNew[exp,x], answer, test, k++];
   NCTest[NCStrongCollectNew[exp,{x}], answer, test, k++];

   exp = y**x + x;
   answer = (1+y)**x;
   NCTest[NCStrongCollectNew[exp,x], answer, test, k++];
   NCTest[NCStrongCollectNew[exp,{x}], answer, test, k++];

   exp = A y**x + B x;
   answer = (B + A y)**x;
   NCTest[NCStrongCollectNew[exp,x], answer, test, k++];
   NCTest[NCStrongCollectNew[exp,{x}], answer, test, k++];

   exp = x**y + x**y;
   answer = 2 x**y;
   NCTest[NCStrongCollectNew[exp,x], answer, test, k++];
   NCTest[NCStrongCollectNew[exp,{x}], answer, test, k++];

   exp = A x**y + B x**y;
   answer = (A + B) x**y;
   NCTest[NCStrongCollectNew[exp,x], answer, test, k++];
   NCTest[NCStrongCollectNew[exp,{x}], answer, test, k++];

   exp = y**x**y + x**y;
   answer = (1 + y) ** x**y;
   NCTest[NCStrongCollectNew[exp,x], answer, test, k++];
   NCTest[NCStrongCollectNew[exp,{x}], answer, test, k++];

   exp = A y**x**y + B x**y;
   answer = (B + A y) ** x**y;
   NCTest[NCStrongCollectNew[exp,x], answer, test, k++];
   NCTest[NCStrongCollectNew[exp,{x}], answer, test, k++];

   exp = y**x**y + y**x;
   answer = y**x**(1 + y);
   NCTest[NCStrongCollectNew[exp,x], answer, test, k++];
   NCTest[NCStrongCollectNew[exp,{x}], answer, test, k++];

   exp = A y**x**y + B y**x;
   answer = y**x**(B + A y);
   NCTest[NCStrongCollectNew[exp,x], answer, test, k++];
   NCTest[NCStrongCollectNew[exp,{x}], answer, test, k++];

   exp = x**x**y + y**x**x + y**x**y + x**y**x;
   answer = x**(x**y + y**x) + y**x**(x + y);
   NCTest[NCStrongCollectNew[exp,x], answer, test, k++];
   NCTest[NCStrongCollectNew[exp,{x}], answer, test, k++];

   answer = x**x**y + y**(x**x + x**y) + x**y**x;
   NCTest[NCStrongCollectNew[exp,y], answer, test, k++];
   NCTest[NCStrongCollectNew[exp,{y}], answer, test, k++];

   exp = x**x**y + y**x**x + y**x**y + x**y**y;
   answer = x**(x**y + y**y) + y**x**(x + y);
   NCTest[NCStrongCollectNew[exp,x], answer, test, k++];
   NCTest[NCStrongCollectNew[exp,{x}], answer, test, k++];

   answer = y**x**(x + y) + x**(x + y)**y;
   NCTest[NCStrongCollectNew[exp,{x,y}], answer, test, k++];

   answer = y**(x**x + x**y) + (x**x + x**y)**y;
   NCTest[NCStrongCollectNew[exp,y], answer, test, k++];
   NCTest[NCStrongCollectNew[exp,{y}], answer, test, k++];

   answer = y**x**(x + y) + x**(x + y)**y;
   NCTest[NCStrongCollectNew[exp,{y,x}], answer, test, k++];

   (* NCDecompose *)

   exp = x**x**y + y**x**x + y**x**y + x**y**x;
   answer = <|{2}->x**x**y + x**y**x + y**x**x,
              {1}->y**x**y|>;
   tmp = NCDecomposeNew[exp,{x}];
   NCTest[tmp, answer, test, k++];
   NCTest[NCComposeNew[tmp], exp, test, k++];
   NCTest[NCComposeNew[tmp, {1}], y**x**y, test, k++];
   NCTest[NCComposeNew[tmp, {2}], x**x**y + x**y**x + y**x**x, test, k++];

   answer = <|{2,1}->x**x**y + x**y**x + y**x**x,
              {1,2}->y**x**y|>;
   tmp = NCDecomposeNew[exp,{x,y}];
   NCTest[tmp, answer, test, k++];
   NCTest[NCComposeNew[tmp], exp, test, k++];
   NCTest[NCComposeNew[tmp, {1,2}], y**x**y, test, k++];
   NCTest[NCComposeNew[tmp, {2,1}], x**x**y + x**y**x + y**x**x, test, k++];

   (* Original tests *)

   exp = tp[a]**b +tp[a]**c;
   answer = tp[a]**(b+c);
   NCTest[NCStrongCollectNew[exp,tp[a]], answer, test, k++];

   exp = tp[a]**(b +c);
   answer = tp[a]**(b+c);
   NCTest[NCStrongCollectNew[exp,tp[a]], answer, test, k++];

   exp = a**b-a**c-a**d;
   answer = a**(b-c-d);
   NCTest[NCStrongCollectNew[exp,a], answer, test, k++];

   exp = a**b-y**c-a**d;
   answer = a**(b-d)-y**c;
   NCTest[NCStrongCollectNew[exp,a], answer, test, k++];

   exp = -j ** b - c ** d ** inv[a] ** b - j ** inv[tp[a]] ** tp[d] ** c + 
     inv[e] ** f ** inv[g] ** h + inv[e] ** f ** tp[f] ** c + 
     tp[h] ** inv[tp[g]] ** inv[g] ** h + tp[h] ** inv[tp[g]] ** tp[f] ** c - 
     c ** d ** inv[a] ** inv[tp[a]] ** tp[d] ** c;
   answer = -c ** (d ** inv[a] ** b + d ** inv[a] ** inv[tp[a]] ** tp[d] ** c) -
     j ** b - (j ** inv[tp[a]] ** tp[d] - inv[e] ** f ** tp[f] -
     tp[h] ** inv[tp[g]] ** tp[f]) ** c + inv[e] ** f ** inv[g] ** h +
     tp[h] ** inv[tp[g]] ** inv[g] ** h;
   NCTest[NCStrongCollectNew[exp,c], answer, test, k++];


];
